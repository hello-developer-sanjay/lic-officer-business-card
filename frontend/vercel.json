{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Expect-CT",
          "value": "max-age=0"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://lic-backend-8jun.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

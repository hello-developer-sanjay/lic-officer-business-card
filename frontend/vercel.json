{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://lic-backend-8jun.onrender.com/api/$1"
    },
    {
      "source": "/",
      "destination": "https://lic-backend-8jun.onrender.com/"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}

{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://lic-backend-8jun.onrender.com/api/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "X-Vercel-Cache",
          "value": "MISS"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}

{
  "rewrites": [
    {
      "source": "/",
      "destination": "https://lic-backend-8jun.onrender.com/"
    },
    {
      "source": "/api/:path*",
      "destination": "https://lic-backend-8jun.onrender.com/api/:path*"
    },
    {
      "source": "/:path+",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
